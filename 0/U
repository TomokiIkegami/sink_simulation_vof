FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            noSlip;
    }
    side
    {
        type            noSlip;
    }

    atmosphere
    {
	type		inletOutlet;
	inletValue	uniform (0 0 0);
	value		uniform (0 0 0);

    }
    atmosphere2
    {
        type            inletOutlet;
	inletValue	uniform (0 0 0);
	value		uniform (0 0 0);
    }
}


