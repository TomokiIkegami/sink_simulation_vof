FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    side
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }


    atmosphere
    {
        type            prghTotalPressure;
        p0              uniform 0;
    }
    atmosphere2
    {
        type            prghTotalPressure;
        p0              uniform 0;
    }


}

